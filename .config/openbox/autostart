# comment/uncomment to disable/enable tint2 &
sleep 1; tint2 -c ~/.config/tint2/raven-grey.tint2rc &

# no desktop icons use nitrogen as wallpaper manager
sleep 1; nitrogen --restore &

# compositor
picom -b &

# PolicyKit authentication
lxpolkit &

# doc menu
plank &

# volume control
volumeicon &

