[begin] (FluxBox)
    [submenu] (Development)
        [exec] (Emacs) {emacs}
    [end]
    [submenu] (Editors)
        [exec] (Mousepad) {mousepad}
        [exec] (Neovim) {st -e nvim}
        [exec] (Vim) {st -e vim}
    [end]
    [submenu] (Graphics)
        [exec] (ImageMagick [color depth=q16]) {display-im6.q16}
        [exec] (Ristretto Image Viewer) {ristretto}
        [exec] (XSane) {xsane}
    [end]
    [submenu] (Multimedia)
        [exec] (Ex Falso) {exfalso}
        [exec] (Parole Media Player) {parole}
        [exec] (PulseAudio Volume Control) {pavucontrol}
        [exec] (Quod Libet) {quodlibet --run}
        [exec] (VLC media player) {vlc}
        [exec] (Xfburn) {xfburn}
    [end]
    [submenu] (Network)
        [exec] (Brave Web Browser) {brave-browser-stable}
        [exec] (Firefox ESR) {firefox-esr}
        [exec] (Hv3 Web Browser) {hv3}
        [exec] (Mail Reader) {exo-open --launch MailReader}
        [exec] (MEGAsync) {megasync}
        [exec] (Modem Manager GUI) {modem-manager-gui}
        [exec] (qBittorrent) {qbittorrent}
        [exec] (uGet) {env GDK_BACKEND=x11 uget-gtk}
        [exec] (Web Browser) {exo-open --launch WebBrowser}
    [end]
    [submenu] (Office)
        [exec] (Atril Document Viewer) {atril}
        [exec] (Dictionary) {xfce4-dict}
        [exec] (LibreOffice Impress) {libreoffice}
        [exec] (mutt) {st -e mutt}
        [exec] (qpdfview) {qpdfview}
    [end]
    [submenu] (Science)
        [exec] (Calc) {st -e calc}
    [end]
    [submenu] (Shells)
        [exec] (kitty) {kitty}
        [exec] (Rxvt Color Unicode Terminal) {urxvt}
        [exec] (Thunar File Manager) {thunar}
        [exec] (UXTerm) {uxterm}
        [exec] (Xfce Terminal) {xfce4-terminal}
        [exec] (XTerm) {xterm}
    [end]
    [submenu] (System)
        [submenu] (Settings)
            [submenu] (GNOME)
                [exec] (Advanced Network Configuration) {nm-connection-editor}
                [exec] (Firewall Configuration) {gufw}
            [end]
            [submenu] (Xfce)
                [exec] (Settings Manager) {xfce4-settings-manager}
            [end]
            [exec] (Accessibility) {xfce4-accessibility-settings}
            [exec] (Appearance) {xfce4-appearance-settings}
            [exec] (Clipboard Manager Settings) {xfce4-clipman-settings}
            [exec] (Color Profiles) {xfce4-color-settings}
            [exec] (Customize Look and Feel) {lxappearance}
            [exec] (Default Applications) {xfce4-mime-settings}
            [exec] (Desktop) {xfdesktop-settings}
            [exec] (Display) {xfce4-display-settings}
            [exec] (File Manager Settings) {thunar-settings}
            [exec] (Keyboard) {xfce4-keyboard-settings}
            [exec] (Kvantum Manager) {kvantummanager}
            [exec] (Mouse and Touchpad) {xfce4-mouse-settings}
            [exec] (nitrogen) {nitrogen}
            [exec] (Notifications) {xfce4-notifyd-config}
            [exec] (Openbox Configuration Manager) {obconf}
            [exec] (Panel) {xfce4-panel --preferences}
            [exec] (Power Manager) {xfce4-power-manager-settings}
            [exec] (Print Settings) {system-config-printer}
            [exec] (Qt5 Settings) {qt5ct}
            [exec] (Removable Drives and Media) {thunar-volman-settings}
            [exec] (Session and Startup) {xfce4-session-settings}
            [exec] (Settings Editor) {xfce4-settings-editor}
            [exec] (Synaptic Package Manager) {synaptic-pkexec}
            [exec] (Tint2 Settings) {tint2conf}
            [exec] (Window Manager) {xfwm4-settings}
            [exec] (Window Manager Tweaks) {xfwm4-tweaks-settings}
            [exec] (Workspaces) {xfwm4-workspace-settings}
            [exec] (Xfce Terminal Settings) {xfce4-terminal --preferences}
        [end]
        [exec] (BleachBit) {bleachbit}
        [exec] (BleachBit [as root]) {pkexec bleachbit}
        [exec] (CPU-X) {cpu-x}
        [exec] (GParted) {/usr/sbin/gparted}
        [exec] (Htop) {st -e htop}
        [exec] (Log Out) {xfce4-session-logout}
        [exec] (Psensor) {psensor}
        [exec] (Sensor Viewer) {xfce4-sensors}
        [exec] (Task Manager) {xfce4-taskmanager}
        [exec] (Tint2) {tint2}
    [end]
    [submenu] (Utilities)
        [exec] (About Xfce) {xfce4-about}
        [exec] (Application Finder) {xfce4-appfinder}
        [exec] (Clipboard Manager) {xfce4-clipman}
        [exec] (File Manager) {exo-open --launch FileManager}
        [exec] (Help) {yelp}
        [exec] (picom) {picom}
        [exec] (Plank) {plank}
        [exec] (Run Program...) {xfce4-appfinder --collapsed}
        [exec] (Screenshot) {xfce4-screenshooter}
        [exec] (Terminal Emulator) {exo-open --launch TerminalEmulator}
        [exec] (Xarchiver) {xarchiver}
    [end]
    [separator]
    [submenu] (FluxBox)
        [workspaces] (Workspaces)
        [submenu] (Styles)
            [stylesdir] (~/.fluxbox/styles)
        [end]
        [config] (Configure)
        [reconfig] (Reconfig)
        [restart] (Restart)
        [separator]
        [exit] (Exit)
    [end]
[end]
